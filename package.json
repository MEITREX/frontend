{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n relay,next -c blue,green \"relay-compiler --watch\" \"next dev -p 3005\"",
    "build": "relay-compiler && next build",
    "start": "next start",
    "lint": "next lint && tsc --noEmit",
    "lint:eslint": "next lint",
    "lint:ts": "tsc --noEmit",
    "relay": "relay-compiler",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "prepare": "husky install",
    "update-schema": "powershell -Command \"fetch-graphql-schema http://localhost:8080/graphql -o src/schema.graphql -r; $c = Get-Content src/schema.graphql; $c = $c -replace 'Assessment,', 'Assessment &'; Set-Content src/schema.graphql $c\""
  },
  "lint-staged": {
    "*.{ts,tsx,css,json}": "prettier --write"
  },
  "dependencies": {
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.15.15",
    "@mui/lab": "5.0.0-alpha.141",
    "@mui/material": "^5.15.15",
    "@mui/x-date-pickers": "^6.19.9",
    "@react-hook/resize-observer": "^1.2.6",
    "@types/is-hotkey": "^0.1.10",
    "@types/lodash": "^4.17.0",
    "@types/node": "18.15.12",
    "@types/pdfobject": "^2.2.5",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@vidstack/react": "^1.12.9",
    "autoprefixer": "10.4.14",
    "clsx": "^2.1.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.10",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "formik": "^2.4.5",
    "framer-motion": "^10.18.0",
    "immutable": "^5.0.3",
    "is-hotkey": "^0.2.0",
    "lodash": "^4.17.21",
    "next": "13.4.7",
    "oidc-client-ts": "^2.4.0",
    "pdfobject": "^2.3.0",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-confetti": "^6.1.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.13",
    "react-hot-toast": "^2.5.2",
    "react-oidc-context": "^2.3.1",
    "react-pdf": "^7.7.1",
    "react-relay": "15.0.0",
    "recharts": "^2.12.5",
    "relay-runtime": "15.0.0",
    "slate": "^0.94.1",
    "slate-history": "^0.93.0",
    "slate-react": "^0.98.4",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4",
    "use-bus": "^2.5.2",
    "usehooks-ts": "^3.1.0",
    "victory": "^36.9.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react-relay": "14.1.3",
    "@types/relay-runtime": "14.1.10",
    "concurrently": "^8.2.2",
    "eslint-config-prettier": "^8.10.0",
    "fetch-graphql-schema": "^0.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "raw-loader": "^4.0.2",
    "relay-compiler": "15.0.0"
  }
}
