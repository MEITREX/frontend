{
  "src": "./",
  "language": "typescript",
  "schema": "./src/schema.graphql",
  "exclude": [
    "**/node_modules/**",
    "**/__mocks__/**",
    "**/__generated__/**",
    "**/.next/**",
    "**/keycloak-theme/**"
  ],
  "artifactDirectory": "./__generated__",
  "customScalars": {
    "Date": "string",
    "DateTime": "string",
    "JSON": "string",
    "LocalTime": "string",
    "Time": "string",
    "Url": "string",
    "UUID": "string",
    "ID": "string"
  }
}
